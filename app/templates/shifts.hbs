<h2>Shifts</h2>


{{#each model as |shift|}}
  <div class=shift_box>
  	{{shift}}
    <div class=check_in> 
      <span class=confirm {{action "checkIn" shift}}>&#10003;</span>
      <span class=deny {{action "noShow" shift}}>&#10005;</span>
    </div>
  </div>
{{/each}}

